{
    "name": "s3-mass-downloader/s3-mass-downloader",
    "type": "library",
    "license": "mit",
    "description": "Solution for bulk files streaming from S3 to ZIP archive",
    "keywords": [
        "s3",
        "aws",
        "downloader",
        "zip",
        "stream"
    ],
    "autoload": {
        "psr-4": {
            "S3MassDownloader\\S3MassDownloader\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marcin Kasprzyk",
            "email": "marcin.kasprzyk@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.283",
        "maennchen/zipstream-php": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
